  <div class="center">
    <ion-card-title style="text-align: center">
      <img style="height: 90px;" src="./assets/logo/logo.svg" />
    </ion-card-title>
    <div class="div-go-back" (click)="goback()">
        <ion-icon style="font-size: 3.7em;float: left;" name="ios-arrow-round-back-outline" class="card-title">
        </ion-icon>
     </div>
    <div style="margin-top: 60px">

      <button ion-button round (click)="loginfb()" class="text-button button-fb">
        Đăng nhập bằng Facebook
      </button>
      <!-- <div style="text-align:center;margin-top: 10px">
        <button ion-button round color="danger" (click)="logingg()"  class="text-button button-gg">
          Đăng nhập bằng Google
        </button>
      </div> -->
      <div style="text-align:center;margin-top: 10px">
        <button ion-button round outline color="light" (click)="logintk()"  class="text-button">Đăng
          nhập bằng tài khoản iVIVU</button>
      </div>
      <!-- <div style="text-align:center;margin-top: 10px">
          <button ion-button round outline color="light" (click)="bypasslogin()" style="text-align:center;width: 100%">Bỏ qua đăng

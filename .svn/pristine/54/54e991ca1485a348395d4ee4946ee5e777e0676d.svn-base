ion-col>\\n            <ion-col text-center  class=\\\"t-center\\\">\\n              <div align=\\\"right\\\">\\n                <img *ngIf=\\\"ischeckroom==true\\\" (click)=\\\"minusroom()\\\" src=\\\"./assets/ic_facility/ic_minus.png\\\">\\n                <img *ngIf=\\\"ischeckroom==false\\\" (click)=\\\"minusroom()\\\" src=\\\"./assets/ic_facility/ic_minus_disable.png\\\">\\n              </div>\\n            </ion-col>\\n            <ion-col text-right class=\\\"t-center\\\">\\n              <img (click)=\\\"plusroom()\\\" src=\\\"./assets/ic_facility/ic_add.png\\\">\\n            </ion-col>\\n          </ion-row>\\n        </ion-item>\\n      </ion-list>\\n    </div>\\n  \\n    <div style=\\\"margin-left: 12px\\\">\\n      <label style=\\\"font-size: 14px;font-weight: bold\\\">\\n        Tuổi trẻ em\\n      </label>\\n      <ion-list *ngFor=\\\"let msg of arrchild\\\">\\n        <ion-item style=\\\"margin-left: -15px\\\">\\n          <ion-label>{{msg.text}}</ion-label>\\n          <ion-select style=\\\"color: #26bed6\\\" [(ngModel)]=\\\"msg.numage\\\" interface=\\\"action-sheet\\\">\\n              <ion-option (ionSelect)=\\\"selectclick($event,msg.text)\\\" value=\\\"<1\\\"><1</ion-option>\\n            <ion-option (ionSelect)=\\\"selectclick($event,msg.text)\\\" value=\\\"1\\\">1</ion-option>\\n            <ion-option (ionSelect)=\\\"selectclick($event,msg.text)\\\" value=\\\"2\\\">2</ion-option>\\n            <ion-option (ionSelect)=\\\"selectclick($event,msg.text)\\\" value=\\\"3\\\">3</ion-option>\\n            <ion-option (ionSelect)=\\\"selectclick($event,msg.text)\\\" value=\\\"4\\\">4</ion-option>\\n            <ion-option (ionSelect)=\\\"selectclick($event,msg.text)\\\" value=\\\"5\\\">5</ion-option>\\n            <ion-option (ionSelect)=\\\"selectclick($event,msg.text)\\\" value=\\\"6\\\">6</ion-option>\\n            <ion-option (ionSelect)=\\\"selectclick($event,msg.text)\\\" value=\\\"7\\\">7</ion-option>\\n            <ion-option (ionSelect)=\\\"
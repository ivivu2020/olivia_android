
    .header-title{
        padding-top: 12px;
        text-align: center;
        font-size: 20px;
        font-weight: bold;
        right: 6%;
    }
    .header-arrow{
        margin: 3px 11px;
    }
    .userprofile-content{
        padding: 16px 16px 16px 16px;
    }
    .div-wrapimg {
        position: relative;
        height: 110px;
        overflow: hidden;
    }
    .img-user-avatar {
        left: 0;
        bottom: 0;
        object-fit: contain;
        max-width: initial;
        background-color: #dedede;
        border-radius: 60px;
        height: 100%;
        
    }
    .text-center{
        text-align: center;
    }
    .text-color{
        color: #26bed6;
    }
    .item-md,.item-ios {
        padding-left: 0 !important;
    }

    .lbl-changepass .item-inner {
        padding-right: 8px;
        border-bottom: none !important;
    }

    .lbl-changepass .checkbox-md {
        margin: 12px 8px 12px 0px !important;
    }

    .btn-cancel{
        border-color: #bdbdbd;
        color: #bdbdbd;
        background: #ffffff;
        width: 46%;
        height: 48px;
        text-transform: none;
        font-size: 17px;
        font-weight: normal;
    }
    
    .btn-cancel:hover{
        border-color: #f79221 !important;
        color: #f79221 !important;
        background: #ffffff !important;
        width: 46%;
        height: 48px;
        text-transform: none;
        font-size: 17px;
        font-weight: normal;
    }
    .btn-save{
        border-color: #f79221 !important;
        color: #ffffff !important;
        background: #f79221 !important;
        width: 44%;
        height: 48px;
        text-transform: none;
        font-size: 17px;
        font-weight: normal;
    }
    .btn-save-phone{
        border-color: #f79221 !important;
        color: #ffffff !important;
        background: #f79221 !important;
        width: 94%;
        height: 48px;
        text-transform: none;
        font-size: 17px;
        font-weight: normal;
    }

    .btn-sendOTP{
        border-color: #f79221 !important;
        color: #ffffff !important;
        background: #f79221 !important;
        width: 94%;
        margin-top: 16px;
        height: 48px;
    }
    .input-otp{
        border: solid 1px #cdcdcd;
        border-radius: 5px;
        width: 100%;
        height: 100%;
    }
    .label-otp{
        margin: 10px 0px 0px 0px;
    }
    .no-padding{
        padding: 0px 8px 0px 0px;
        align-self: center;
        text-align: right;
    }
    .row-otp{
        height: 32px;
        margin-top: 16px;
    }
    .label-sent-otp{
        font-size: 12px;
        color: #848484;
        white-space: normal;
    }
    .label-warning{
        font-size: 12px;
        color: red !important;
        white-space: normal;
    }

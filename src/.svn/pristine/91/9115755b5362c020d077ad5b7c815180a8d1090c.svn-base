::ng-deep{
    .searchbar-input.sc-ion-searchbar-md{
        box-shadow: none;
        padding-inline-start: 0;
        padding-inline-end: 0;
        padding-top: 4px;
        padding-bottom: 2px;
    }

    .sc-ion-searchbar-md-h {
        -webkit-padding-start: 0;
        padding-inline-start: 0;
        -webkit-padding-end: 0;
        padding-inline-end: 0;
    }
}
}

.item-float-search{
    height: 48px;
    position: absolute;
    width: 100%;
    top: 0;
    left: 48px;
}

.float-search-white-bar{
    position: absolute;
    z-index: 1002;
    width: 100%;
    background-color: white;
    height: 114px;
    box-shadow: 0px 1px 2px 0px #bdbdbd;
    //visibility: hidden;
}

.experiencesearch-content{
    padding: 16px;
    background-color: #ffffff;

    

    .div-map{
        width: 100%;
        height: calc(100vh - env(safe-area-inset-top) - 80px);
        overflow: hidden;
    }

    .div-float-item-search{
        overflow-x: auto;
        overflow-y: hidden;
        //z-index: 998;
        background: transparent;
        //bottom: calc(80px + env(safe-area-inset-bottom) - 22px);
        position: fixed;
        height: 104px;
        width: 100%;
        justify-content: space-between;
        display: flex;
        z-index: 12;
        bottom: 90px;
    }    
    .float-item-search{
        background: #ffffff;
        border-radius: 6px;
        position: relative;
        display: table-cell;
        padding-left: 8px;
        height: 104px;
        margin-left: 16px;
    }
    .float-item-search:last-child{
        background: #ffffff;
        border-radius: 6px;
        position: relative;
        display: table-cell;
        padding-left: 8px;
        height: 104px;
        margin-left: 16px;
        margin-right: 16px;
    }
}


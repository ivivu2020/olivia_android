        /* The popup bubble styling. */
  .popup-bubble {
    /* Position the bubble centred-above its parent. */
    position: absolute;
    top: 0;
    left: 0;
    transform: translate(-50%, -100%);
    /* Style the bubble. */
    background-color: #ffffff;
    border:solid 0.5px #ffffff;
    padding: 5px 10px;
    border-radius: 10px;
    font-family: sans-serif;
    white-space: nowrap;
    background: #003c71;
    color: #ffffff;
    font-size: 10px;
    font-family: Roboto;
    letter-spacing: 0.12px;
    height: 22px;
  }
  /* The parent of the bubble. A zero-height div at the top of the tip. */
  .popup-bubble-anchor {
    /* Position the div a fixed distance above the tip. */
    position: absolute;
    width: 100%;
    bottom: 8px;
    left: 0;
  }
  
  /* JavaScript will position this div at the bottom of the popup tip. */
  .popup-container {
    cursor: auto;
    height: 0;
    position: absolute !important;
    width: 10px;
  }
  .popup-selected{
    background: #ffffff;
    color: #003c71;
    border:solid 1px #003c71;
    z-index: 99;
    font-size: 13px;
    letter-spacing: 0.16px;
    padding: 2px 10px;
    font-family: Roboto;
    font-weight:bold;
  }


  div.popup-bubble-anchor::after{
    content: '';
    display: block;
    position: absolute;
    left: 100%;
    width: 0;
    height: 0;
    border-style: solid;

    top: 10px;
    border-color: transparent transparent transparent #fdd;
    border-width: 10px;
  }
}

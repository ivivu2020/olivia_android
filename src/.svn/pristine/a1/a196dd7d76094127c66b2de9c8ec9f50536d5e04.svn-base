.experiencesearch-content{
    padding: 16px;
    background-color: #ffffff;
    height: 100%;
    .div-map{
        width: 100%;
        height: 812px;
        overflow: hidden;
        top: -150px;
    }

    .float-search{
        height: 48px;
        background-color: #ffffff;
        background: #fff;
        position: fixed;
        width: 91.5%;
        margin: 24px 16px;
        border-radius: 4px;
        border: solid 0.5px #bdbdbd;
        z-index: 999;
    }
    
    .input-search-experiencesearch{
        color: #000000;
        padding-top: 0;
        padding-bottom: 0;
        width: 98%;
        -webkit-padding-start: 0px;
        padding-inline-start: 0px;
        -webkit-padding-end: 0px;
        padding-inline-end: 0px;
    
    }
    
    .item-float-search{
        height: 46px;
    }

    .div-float-item-search{
        overflow-x: auto;
        overflow-y: hidden;
        z-index: 998;
        background: transparent;
        bottom: 88px;
        position: fixed;
        height: 104px;
        width: 100%;
        justify-content: space-between;
        display: flex;
        
    }    
    .float-item-search{
        background: #ffffff;
        border-radius: 6px;
        position: relative;
        display: table-cell;
        padding-left: 8px;
        height: 104px;
        margin-left: 16px;
    }
    .float-item-search:last-child{
        background: #ffffff;
        border-radius: 6px;
        position: relative;
        display: table-cell;
        padding-left: 8px;
        height: 104px;
        margin-left: 16px;
        margin-right: 16px;
    }

}


    .div-statusbar-float{
      position: absolute;
      z-index: 12;
      width: 100%;
      background-color: #ffffff;
      color: #000000;
      padding-left: 16px;
      visibility: hidden;
      opacity: 0;
      transition: visibility 0.2s, opacity 0.2s linear;
  }
  .float-statusbar-enabled{
      visibility: visible;
      transition: visibility 0.2s, opacity 0.2s linear;
      opacity: 1;
      height: calc(env(safe-area-inset-top)*2 - 22px );
  }
  .float-statusbar-disabled{
      visibility: hidden;
      opacity: 0;
      transition: visibility 0.2s, opacity 0.2s linear;
      z-index: 0;
      height: 0px;
  }
